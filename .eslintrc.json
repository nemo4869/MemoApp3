{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/function-component-definition": [
            2, { "namedComponents": "function-declaration" }
        ],
        "no-use-before-define":0
    }
}
